{
  "name": "a-simple-store",
  "version": "2.0.3",
  "description": "A Simple Store manager that is suitable for javascript frontend applications",
  "main": "dist/a-simple-store.cjs.js",
  "module": "dist/a-simple-store.esm.js",
  "browser": "dist/a-simple-store.umd.js",
  "scripts": {
    "build": "rollup -c",
    "pretest": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/viacheslav-dev-1/simple-store.git"
  },
  "keywords": [
    "store",
    "state",
    "manager",
    "subject",
    "interval"
  ],
  "author": " <bviacheslav.12@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/viacheslav-dev-1/simple-store/issues"
  },
  "homepage": "https://github.com/viacheslav-dev-1/simple-store#readme",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/a-simple-store.esm.js",
        "require": "./dist/a-simple-store.cjs.js"
      },
      "node": {
        "require": "./dist/a-simple-store.cjs.js"
      },
      "default": "./dist/a-simple-store.esm.js"
    }
  },
  "dependencies": {
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.5",
    "@babel/preset-env": "^7.21.5",
    "rollup": "^2.79.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
